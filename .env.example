# eBay Keys (Get these from developer.ebay.com)
EBAY_APP_ID=YOUR_APP_ID_HERE
EBAY_CERT_ID=YOUR_CERT_ID_HERE
EBAY_DEV_ID=YOUR_DEV_ID_HERE

# Ollama Configuration (Legacy - kept for backward compatibility)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3

# Agent-Specific LLM Configuration
# Providers: "ollama", "groq", or "gemini"
# See plan docs for recommended models per agent

# Triage Agent (Binary classification - needs accuracy)
TRIAGE_PROVIDER=ollama
TRIAGE_MODEL=gemma2:9b-instruct

# Planner Agent (Creative query generation - needs strategy)
PLANNER_PROVIDER=ollama
PLANNER_MODEL=llama3.1:8b-instruct

# Niche Discovery Agent (Market analysis - optional)
NICHE_PROVIDER=ollama
NICHE_MODEL=mixtral:8x7b-instruct-v0.1
NICHE_USE_LLM=false

# Reflection Agent (Quality assurance - reviews deals for red flags)
REFLECTION_PROVIDER=ollama
REFLECTION_MODEL=gemma2:9b-instruct
REFLECTION_ENABLED=true

# Groq API Key (if using Groq provider)
# GROQ_API_KEY=gsk_...

# Google API Key (if using Gemini provider)
# GOOGLE_API_KEY=...

# Logging
LOG_LEVEL=INFO
